int am1=6;
int rm1=7;
int am2=4;
int rm2=3;

void setup()
{
  pinMode (am1,OUTPUT);
  pinMode (rm1,OUTPUT);
  pinMode (am2,OUTPUT);
  pinMode (am2,OUTPUT);
}

void loop()
{
  digitalWrite(am1,HIGH);
  digitalWrite(am2,HIGH);
  delay(5000);
  digitalWrite(am1,LOW);
  digitalWrite(am1,LOW);
  delay(2000);
}
